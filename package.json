{
  "name": "claude-mcp-server",
  "version": "0.1.0",
  "description": "Comprehensive ecosystem of universal MCP servers that enhance Claude's abilities",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "node orchestration/dist/index.js",
    "dev": "concurrently \"npm run dev:orchestration\" \"npm run dev:servers\"",
    "dev:orchestration": "nodemon orchestration/src/index.js",
    "dev:servers": "concurrently \"npm run dev:inference\" \"npm run dev:ui-testing\" \"npm run dev:analytics\" \"npm run dev:code-quality\" \"npm run dev:documentation\" \"npm run dev:memory\" \"npm run dev:web-access\"",
    "dev:inference": "nodemon servers/inference-enhancement/src/server.js",
    "dev:ui-testing": "nodemon servers/ui-testing/src/server.js",
    "dev:analytics": "nodemon servers/analytics/src/server.js",
    "dev:code-quality": "nodemon servers/code-quality/src/server.js",
    "dev:documentation": "nodemon servers/documentation/src/server.js",
    "dev:memory": "nodemon servers/memory-management/src/server.js",
    "dev:web-access": "nodemon servers/web-access/src/server.js",
    "build": "npm run build:orchestration && npm run build:servers",
    "build:orchestration": "tsc --project orchestration/tsconfig.json",
    "build:servers": "concurrently \"npm run build:inference\" \"npm run build:ui-testing\" \"npm run build:analytics\" \"npm run build:code-quality\" \"npm run build:documentation\" \"npm run build:memory\" \"npm run build:web-access\"",
    "build:inference": "tsc --project servers/inference-enhancement/tsconfig.json",
    "build:ui-testing": "tsc --project servers/ui-testing/tsconfig.json",
    "build:analytics": "tsc --project servers/analytics/tsconfig.json",
    "build:code-quality": "tsc --project servers/code-quality/tsconfig.json",
    "build:documentation": "tsc --project servers/documentation/tsconfig.json",
    "build:memory": "tsc --project servers/memory-management/tsconfig.json",
    "build:web-access": "tsc --project servers/web-access/tsconfig.json",
    "test": "jest",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration",
    "test:e2e": "jest tests/e2e",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .ts,.js",
    "lint:fix": "eslint . --ext .ts,.js --fix",
    "format": "prettier --write .",
    "db:migrate": "node scripts/db-migrate.js",
    "db:seed": "node scripts/db-seed.js",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "start-orchestration": "node scripts/start-orchestration.js",
    "configure:claude-desktop": "node scripts/configure-claude-desktop.js",
    "configure:claude-code": "node scripts/configure-claude-code.js"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "claude",
    "ai",
    "assistant",
    "orchestration",
    "inference",
    "testing",
    "analytics",
    "code-quality",
    "documentation",
    "memory-management",
    "web-access"
  ],
  "author": "Claude MCP Server Team",
  "license": "MIT",
  "dependencies": {
    "@types/node": "^20.11.24",
    "axios": "^1.6.7",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "knex": "^3.1.0",
    "pg": "^8.11.3",
    "redis": "^4.6.13",
    "ws": "^8.16.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/pg": "^8.11.2",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/your-org/claude-mcp-server.git"
  },
  "bugs": {
    "url": "https://github.com/your-org/claude-mcp-server/issues"
  },
  "homepage": "https://github.com/your-org/claude-mcp-server#readme"
}